package com.bsi.dms.config;

import android.media.AudioManager;
import android.os.PowerManager;
import android.view.View;
import android.webkit.WebView;

public class PlayerConfig {
	private static int maxVolume;
	private static int currentVolume;
	private static int width;
	private static int height;
	private static int rate;
	//private  AudioManager audiomanage; 
	public static PowerManager pManager;
	public static View view;
	
	public PlayerConfig(){}
	
	public PlayerConfig(int maxVolume, int currentVolume, int width, int height, int rate) {
		this.maxVolume = maxVolume;
		this.currentVolume = currentVolume;
		this.width = width;
		this.height = height;
		this.rate = rate;
	}
	
	public static int getMaxVolume() {
		return maxVolume;
	}
	public static void setMaxVolume(int maxVolume) {
		PlayerConfig.maxVolume = maxVolume;
	}
	
	public static int getCurrentVolume() {
		return currentVolume;
	}
	public static void setCurrentVolume(int currentVolume) {
		PlayerConfig.currentVolume = currentVolume;
	}
	
	public static int getWidth() {
		return width;
	}
	public static void setWidth(int width) {
		PlayerConfig.width = width;
	}
	
	public static int getHeight() {
		return height;
	}
	public static void setHeight(int height) {
		PlayerConfig.height = height;
	}
	
	public static int getRate() {
		return rate;
	}
	public static void setRate(int rate) {
		PlayerConfig.rate = rate;
	}

}
