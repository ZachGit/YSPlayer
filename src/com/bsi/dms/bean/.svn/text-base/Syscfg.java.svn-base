package com.bsi.dms.bean;

public class Syscfg {
	private String mac = "";
	private String serverip = "";
	private String serverport = "";
	private String playid = "";
	private Integer heartbeattime = 30;
	private String baseurl = "";
	private String equipmentId = "";
	private String serverVirtualDir = "";

	public Syscfg(){}
	
	public Syscfg(String serverip, String serverport, String playid, Integer heartbeattime) {
		this.serverip = serverip;
		this.serverport = serverport;
		this.playid = playid;
		this.heartbeattime = heartbeattime;
	}
	
	public String getServerip() {
		return serverip;
	}

	public void setServerip(String serverip) {
		this.serverip = serverip;
	}
	
	public String getServerport() {
		return serverport;
	}

	public void setServerport(String serverport) {
		this.serverport = serverport;
	}

	public String getPlayid() {
		return playid;
	}

	public void setPlayid(String playid) {
		this.playid = playid;
	}
	
	public Integer getHeartbeattime() {
		return heartbeattime;
	}

	public void setHeartbeattime(Integer heartbeattime) {
		this.heartbeattime = heartbeattime;
	}

	@Override
	public String toString() {
		return "serverip="+ serverip+ ",serverport="+ serverport+ ",playid="+ playid + ",heartbeattime="+ heartbeattime;
	}

	public String getBaseurl() {
		return baseurl;
	}

	public void setBaseurl(String baseurl) {
		this.baseurl = baseurl;
	}

	public String getMac() {
		return mac;
	}

	public void setMac(String mac) {
		this.mac = mac;
	}

	public String getEquipmentId() {
		return equipmentId;
	}

	public void setEquipmentId(String equipmentId) {
		this.equipmentId = equipmentId;
	}

	public String getServerVirtualDir() {
		return serverVirtualDir;
	}

	public void setServerVirtualDir(String serverVirtualDir) {
		this.serverVirtualDir = serverVirtualDir;
	}

}
