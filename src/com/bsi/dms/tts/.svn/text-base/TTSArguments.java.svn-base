package com.bsi.dms.tts;

/**
 * Wrap tts arguments
 * @author Zuoshu
 *
 */
public class TTSArguments {
	// tts string
	public String text;
	// whether cancel previous tts data
	public boolean cancelPrevious;
	// generated from text
	public String ttsId;
	// callback handle tts sound data
	public TTSServiceCallback callback;

	public TTSArguments(String text, boolean cancelPrevious, String ttsId,
			TTSServiceCallback callback) {
		this.text = text;
		this.cancelPrevious = cancelPrevious;
		this.ttsId = ttsId;
		this.callback = callback;
	}
}
